server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /var/www/removeddit;
	index index.html;
	server_name _;

	location / {
		try_files /index.html /index.html =404;
	}

	location /r/ {
		try_files /thread.html /thread.html =404;
	}

	location /static/ {
		try_files $uri $uri/ =404;
	}
}